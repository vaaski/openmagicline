{
  "name": "openmagicline",
  "description": "Magicline API for everybody.",
  "keywords": [
    "magicline",
    "gym",
    "mysports",
    "reverse-engineered"
  ],
  "version": "0.1.2",
  "main": "lib/index.js",
  "license": "MIT",
  "author": "vaaski <admin@vaa.ski>",
  "scripts": {
    "start": "node -r dotenv/config lib",
    "build": "rimraf lib && tsc",
    "prepare": "npm run build",
    "dev": "nodemon",
    "test": "nyc ava -s",
    "test:full": "rimraf token.json && npm test",
    "test:watch": "nodemon -w src -w tests -w types --exec npm test",
    "coverage": "live-server coverage/lcov-report --port=8069",
    "lint": "eslint . --fix --ext ts",
    "format": "prettier -w types/* tests/* src/*"
  },
  "files": [
    "lib/**/*",
    "types/**/*"
  ],
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "tests/**/*.test.ts"
    ],
    "require": [
      "dotenv/config"
    ],
    "nodeArguments": [
      "--loader=tsx"
    ],
    "failFast": true,
    "verbose": true
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/lodash": "^4.14.170",
    "@types/node": "^18.11.9",
    "@types/ws": "^7.4.7",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "ava": "^5.1.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ava": "^13.2.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^44.0.2",
    "fs-jetpack": "^4.1.0",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "tsx": "^3.12.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@stomp/stompjs": "^6.1.0",
    "axios": "^0.21.1",
    "debug": "^4.3.1",
    "form-data": "^4.0.0",
    "lodash": "^4.17.21",
    "ws": "^7.5.3"
  }
}
